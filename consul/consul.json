{
  "datacenter": "dc1",
  "data_dir": "/consul/data",
  "bind_addr": "0.0.0.0",
  "client_addr": "0.0.0.0",
  "server": true,
  "bootstrap_expect": 1,
  "ui_config": {
    "enabled": true
  },
  "connect": {
    "enabled": true
  },
  "ports": {
    "grpc": 8502
  },
  "log_level": "INFO",
  "enable_script_checks": true,
  "disable_remote_exec": true,
  "acl": {
    "enabled": false,
    "default_policy": "allow"
  },
  "performance": {
    "raft_multiplier": 1
  },
  "retry_join": [],
  "enable_local_script_checks": true
}
